{
	"private": true,
	"scripts": {
		"clean": "rm -r dist/ && rm LavaPlace.js",
		"clean:win": "rmdir /s /q dist && del LavaPlace.js",
		"build:noclean": "tsc && node utils/build.js && prettier LavaPlace.js --write --config .prettierrc",
		"build": "npm run clean && npm run build:noclean",
		"build:win": "npm run clean:win && npm run build:noclean"
	},
	"devDependencies": {
		"@types/node": "^24.3.1",
		"@types/tampermonkey": "^5.0.4",
		"prettier": "^3.6.2",
		"typescript": "^5.9.2"
	}
}
